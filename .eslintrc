{
  "env": {
    "jquery": true,
    "mocha": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-unused-expressions": 2,
    "strict": 0,
    "wrap-iife": [
      2,
      "any"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 0,
    "new-cap": 2,
    "no-caller": 2,
    "require-yield": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2
  }
}
